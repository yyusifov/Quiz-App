<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ADFF00"
    tools:context=".Result_Activity">

    <ImageView
        android:id="@+id/result_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="92dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/_599805_223179_p1x4gu_250_1" />

    <Button
        android:id="@+id/feedback_button"
        android:layout_width="250dp"
        android:layout_height="70dp"
        android:layout_marginTop="52dp"
        android:backgroundTint="#FFFFFF"
        android:elevation="5dp"
        android:fontFamily="@font/koulen"
        android:stateListAnimator="@null"
        android:text="Feedback"
        android:textColor="#000000"
        android:textSize="28sp"
        app:cornerRadius="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/result_image"
        app:strokeColor="#000000"
        app:strokeWidth="1dp" />

    <Button
        android:id="@+id/mainpage_button"
        android:layout_width="250dp"
        android:layout_height="70dp"
        android:layout_marginTop="32dp"
        android:backgroundTint="#263238"
        android:elevation="5dp"
        android:fontFamily="@font/koulen"
        android:stateListAnimator="@null"
        android:text="Main Page"
        android:textColor="#FFFFFF"
        android:textSize="28sp"
        app:cornerRadius="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/feedback_button" />
</androidx.constraintlayout.widget.ConstraintLayout>